{
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx" : true
        }
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "extends":[
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
        "@typescript-eslint",
        "react-hooks",
        "import"
    ]
}